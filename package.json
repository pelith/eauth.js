{
  "name": "eauth.js",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --format cjs,esm,umd --name Eauth",
    "build": "tsdx build --format cjs,esm,umd --name Eauth",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --format cjs,esm,umd --name Eauth",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Pelith",
  "module": "dist/eauth.esm.js",
  "size-limit": [
    {
      "path": "dist/eauth.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/eauth.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.2",
    "husky": "^7.0.1",
    "size-limit": "^5.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {}
}
